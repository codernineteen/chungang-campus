{
  "name": "server-udp",
  "version": "0.1.1",
  "description": "server implementation with udp protocol using geckos.io library",
  "main": "server.js",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:nodejs": "nodemon src/server.mjs",
    "dev:browser": "live-server --port=8080",
    "dev:esbuild": "esbuild js/index.js --outfile=js/bundle.js --bundle --watch",
    "build": "esbuild js/index.js --outfile=js/bundle.js --bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/codernineteen/chungang-campus.git"
  },
